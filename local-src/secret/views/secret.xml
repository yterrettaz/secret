<?xml version="1.0" encoding="utf-8"?>
<odoo>


<!-- form.view -->
<record id="sc_view_secret_secret_form" model="ir.ui.view">
    <field name="name">form.secret.secret</field>
    <field name="model">secret.secret</field>
    <field name="arch" type="xml">
        <form>
            <header>
            </header>
            <sheet>
                <group>
                    <field name="secret"/>
                    <field name="name"/>
                    <field name="ipaddress"/>
                </group>
            </sheet>
        </form>
    </field>
</record>

<!-- tree.secret -->
<record id="sc_view_secret_secret_tree" model="ir.ui.view">
    <field name="name">tree.secret.secret</field>
    <field name="model">secret.secret</field>
    <field name="arch" type="xml">
	    <tree>
	        <field name="name"/>
            <field name="create_date"/>
	    </tree>
    </field>
</record>

</odoo>